{
  "name": "gae-init",
  "dependencies": {
    "bootstrap": "~3",
    "flag-icon-css": "~0",
    "font-awesome": "~4",
    "jquery": "~2",
    "momentjs": "~2",
    "nprogress": "https://github.com/lipis/nprogress.git#master"
  },
  "exportsOverride": {
    "bootstrap": {
      "js": "js/*.js",
      "less": "less/*.less",
      "font": "fonts/*"
    },
    "flag-icon-css": {
      "less": "less/*.less",
      "flag/1x1": "flags/1x1/{gb,gr,pl,ru}.svg",
      "flag/4x3": "flags/1x1/{gb,gr,pl,ru}.svg"
    },
    "font-awesome": {
      "less": "less/*.less",
      "font": "fonts/*"
    },
    "jquery": {
      "js": "dist/jquery.js"
    },
    "momentjs": {
      "js": "*.js",
      "js/lang": "lang/*.js"
    },
    "nprogress": {
      "js": "*.js",
      "less": "*.less"
    }
  }
}
